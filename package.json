{
  "name": "tree-node-data",
  "version": "1.0.0",
  "description": "Provides tree node meta data useful for building tree structure UI for data display and data traversal",
  "main": "lib/tree-node-data.js",
  "scripts": {
    "build": "webpack --env dev && webpack --env build",
    "build:demo": "webpack --env dev && webpack --config webpack.config.demo.js",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "mocha --require babel-core/register --colors ./src/*.spec.js",
    "test:watch": "mocha --require babel-core/register --colors -w ./src/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/linsight/tree-node-data.git"
  },
  "keywords": [
    "tree",
    "structure",
    "traversal",
    "descendants",
    "ancestors",
    "data"
  ],
  "author": "David Lin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/linsight/tree-node-data/issues"
  },
  "homepage": "https://github.com/linsight/tree-node-data",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "eslint": "^4.13.1",
    "eslint-loader": "^1.9.0",
    "html-webpack-plugin": "^3.0.4",
    "mocha": "^4.0.1",
    "webpack": "^3.10.0",
    "yargs": "^11.0.0"
  }
}
